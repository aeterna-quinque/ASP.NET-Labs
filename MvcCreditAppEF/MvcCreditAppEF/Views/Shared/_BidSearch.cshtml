@model IEnumerable<MvcCreditAppEF.Models.Bid>

<div>
    <h4> Все заявители кредита "@Model.First().CreditHead"</h4>
    <ul>
        @{if (Model.Count() != 0)
            {
                foreach (var item in Model)
                {
                    <li>@item.Name</li>
                }
            }
        }
    </ul>
</div>